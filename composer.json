{
    "name": "goldLeafHouse",
    "description": "social Community with goldLeaf",
    "keywords": ["social", "goldLeaf"],
    "homepage": "http://www.nxjyjy.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/shehuaqigai/goldleafhouse/issues",
        "wiki": "https://github.com/shehuaqigai/goldleafhouse/wiki",
        "source": "https://github.com/shehuaqigai/goldleafhouse"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "bower-asset/conditionizr": "dev-master",
        "yiisoft/yii2-swiftmailer": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
